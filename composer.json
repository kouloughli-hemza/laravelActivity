{
    "name": "kouloughliapp/activity-log",
    "description": "User activity log plugin for Kouloughli.",
    "keywords": [
        "laravel",
        "Kouloughli",
        "activity log",
        "user activity"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Kouloughli Hemza",
            "email": "php7@outlook.fr",

            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "kouloughliapp/plugins": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "kouloughli\\UserActivity\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "kouloughli\\UserActivity\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
